{
  "name": "starter-template",
  "type": "module",
  "version": "0.9.8",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.css\" --fix",
    "codestyle": "pnpm lint && pnpm stylelint && pnpm type-check",
    "story": "histoire dev --port 3001",
    "openapi-generate": "openapi-typescript https://myapi.dev/api/v1/openapi.yaml -o ./src/shared/api/schema/index.d.ts"
  },
  "dependencies": {
    "modern-normalize": "^2.0.0",
    "openapi-fetch": "^0.8.2",
    "pinia": "^2.1.7",
    "radix-vue": "^1.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@csstools/postcss-global-data": "^2.1.0",
    "@histoire/plugin-vue": "^0.17.6",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "histoire": "^0.17.6",
    "npm-run-all2": "^6.1.1",
    "openapi-typescript": "^6.7.2",
    "postcss": "^8.4.32",
    "postcss-custom-media": "^10.0.2",
    "postcss-load-config": "^5.0.2",
    "postcss-nested": "^6.0.1",
    "stylelint": "^16.0.1",
    "stylelint-config-standard": "^35.0.0",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^1.8.19"
  }
}
